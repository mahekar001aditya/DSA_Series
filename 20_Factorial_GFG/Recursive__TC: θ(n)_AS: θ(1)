class Solution {
    public static int factorial(int n) {      // n = 4
                        
        int res = 1;                         // res = 0 | 1 => return res = 1;
        
        for(int i = 2 ;i<= n; i++){
            res = res * i;                //  1! * 2! * 3! * 4!                  
        }
        return res;
    }
}

